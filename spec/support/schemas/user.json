{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id",
    "email_address",
    "opt_in_status",
    "first_name",
    "last_name",
    "address"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "default": "",
      "examples": [
        "c4ca4238a0b923820dcc509a6f75849b"
      ],
      "pattern": "^(.*)$"
    },
    "email_address": {
      "$id": "#/properties/email_address",
      "type": "string",
      "title": "The Email_address Schema",
      "default": "",
      "examples": [
        "spree1@example.com"
      ],
      "pattern": "^(.*)$"
    },
    "opt_in_status": {
      "$id": "#/properties/opt_in_status",
      "type": "boolean",
      "title": "The Opt_in_status Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "first_name": {
      "$id": "#/properties/first_name",
      "type": "string",
      "title": "The First_name Schema",
      "default": "",
      "examples": [
        "John 1"
      ],
      "pattern": "^(.*)$"
    },
    "last_name": {
      "$id": "#/properties/last_name",
      "type": "string",
      "title": "The Last_name Schema",
      "default": "",
      "examples": [
        "Doe1"
      ],
      "pattern": "^(.*)$"
    },
    "address": {
      "$id": "#/properties/address",
      "type": "object",
      "title": "The Address Schema",
      "required": [
        "address1",
        "address2",
        "city",
        "province",
        "province_code",
        "postal_code",
        "country",
        "country_code"
      ],
      "properties": {
        "address1": {
          "$id": "#/properties/address/properties/address1",
          "type": "string",
          "title": "The Address1 Schema",
          "default": "",
          "examples": [
            "123 street"
          ],
          "pattern": "^(.*)$"
        },
        "address2": {
          "$id": "#/properties/address/properties/address2",
          "type": "string",
          "title": "The Address2 Schema",
          "default": "",
          "examples": [
            "12"
          ],
          "pattern": "^(.*)$"
        },
        "city": {
          "$id": "#/properties/address/properties/city",
          "type": "string",
          "title": "The City Schema",
          "default": "",
          "examples": [
            "dakota"
          ],
          "pattern": "^(.*)$"
        },
        "province": {
          "$id": "#/properties/address/properties/province",
          "type": "string",
          "title": "The Province Schema",
          "default": "",
          "examples": [
            "North Dakota"
          ],
          "pattern": "^(.*)$"
        },
        "province_code": {
          "$id": "#/properties/address/properties/province_code",
          "type": "string",
          "title": "The Province_code Schema",
          "default": "",
          "examples": [
            "ND"
          ],
          "pattern": "^(.*)$"
        },
        "postal_code": {
          "$id": "#/properties/address/properties/postal_code",
          "type": "string",
          "title": "The Postal_code Schema",
          "default": "",
          "examples": [
            "14523"
          ],
          "pattern": "^(.*)$"
        },
        "country": {
          "$id": "#/properties/address/properties/country",
          "type": "string",
          "title": "The Country Schema",
          "default": "",
          "examples": [
            "United States"
          ],
          "pattern": "^(.*)$"
        },
        "country_code": {
          "$id": "#/properties/address/properties/country_code",
          "type": "string",
          "title": "The Country_code Schema",
          "default": "",
          "examples": [
            "US"
          ],
          "pattern": "^(.*)$"
        }
      }
    }
  }
}
